{
  "mcpServers": {
    "browser-tools": {
      "command": "npx",
      "args": [
        "@agentdeskai/browser-tools-mcp@latest"
      ],
      "env": {
        "BROWSER_TOOLS_HOST": "127.0.0.1",
        "BROWSER_TOOLS_PORT": "3025"
      }
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ]
    },
    "promptx": {
      "command": "npx",
      "args": [
        "-y",
        "-f",
        "dpml-prompt@snapshot",
        "mcp-server"
      ],
      "env": {
        "PROMPTX_WORKSPACE": "C:\\Users\\Administrator\\Downloads\\幻境之旅生成器 (4)"
      }
    }
  }
}